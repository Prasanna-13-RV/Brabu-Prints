	<%- include('./partials/header.ejs') %>
		<div class="containerCarousel1">
			<div class="containerCarouselH1">
				<h1>G<span>alle</span>ry</h1>
			</div>
			<div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
				<div class="carousel-inner">
					<% galleryArray.forEach(galleryFor=> { %>
						<% for(let i=0; i <=data.length - 1; i++) { %>
							<div class="carousel-item <%= i===0 ? " active" : "" %>">
								<img class="classCarouselgallery" src="<%= galleryFor.gallery_image %>" alt="">
							</div>
							<% } %>
								<% }) %>
				</div>
				<button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls"
					data-bs-slide="prev">
					<span class="carousel-control-prev-icon" aria-hidden="true"></span>
					<span class="visually-hidden">Previous</span>
				</button>
				<button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls"
					data-bs-slide="next">
					<span class="carousel-control-next-icon" aria-hidden="true"></span>
					<span class="visually-hidden">Next</span>
				</button>
			</div>
		</div>


		<div class="containerCarousel2">
			<div class="containerCarouselH1">
				<h1>Ou<span>r Post</span>ers</h1>
			</div>
			<div id="myCarousel" class="carousel slide" data-ride="carousel">
				<!-- Wrapper for slides -->
				<div class="carousel-inner">
					<% clientArray.forEach(clientFor=> { %>
						<% for(let i=0; i <=data.length - 1; i++) { %>
							<div class="item <%= i===0 ? " active" : "" %>">
								<img class="classCarouselposter" src="<%= clientFor.client_poster %>" alt="">
							</div>
							<% } %>
								<% }) %>
				</div>

				<!-- Left and right controls -->
				<a class="left carousel-control" href="#myCarousel" data-slide="prev">
					<span class="glyphicon glyphicon-chevron-left"></span>
					<span class="sr-only">Previous</span>
				</a>
				<a class="right carousel-control" href="#myCarousel" data-slide="next">
					<span class="glyphicon glyphicon-chevron-right"></span>
					<span class="sr-only">Next</span>
				</a>
			</div>
		</div>


